{
  "name": "Ansible + Kubernetes + OpenTofu",
  "image": "mcr.microsoft.com/devcontainers/python:1-${templateOption:imageVariant}",
  "customizations": {
    "vscode": {
      "extensions": [
        "opentofu.vscode-opentofu",
